    },
    {
      "start_time": 71,
      "end_time": 72,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Just like hopes springing high,"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Tempo"
        },
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        }
